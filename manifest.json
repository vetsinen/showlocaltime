{
  "background": {
    "scripts": [
      "background.js",
      "timeconvertor.js"
    ]
  },
  "permissions": [
    "tabs", "<all_urls>"
  ],
  "icons": {
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "Click to change",
    "default_icon": "icon128.png"
  },
  "manifest_version": 2,
  "name": "ShowLocalTime",
  "description": "show local time for event on meetup etc.",
  "version": "0.1.1"
}